<div class="clubLink">
  <div class="plusContainer">
    <div class="inputForm">

    <h1><%= @club.name %> Membership</h1>

    <%= form_for(:membership,url: '/clubusers/update') do |f| %>
    <div class="field"> 
      <ul>
         <li><%= f.select :id, options_for_select(@membershipList,selected: @clubUser.membership_id) %></li>
          <%= f.hidden_field :club_user, value: @clubUser.id %>  
       </ul>
  	</div> 
  	<div class="actions"> 
      <%= f.submit "Update", class: "form-btn" %> 
  	</div> 
    <% end %>
 

    </div>
  </div>
</div>